<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <Authors>SSRD d.o.o.</Authors>
    <Company>SSRD d.o.o.</Company>
    <PackageProjectUrl>https://github.com/ssrdio/RevolutAPI</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/ssrdio/RevolutAPI/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/ssrdio/RevolutAPI</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Revolut, OpenAPI, Revolut API client</PackageTags>
    <Version>3.2.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="README.md" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="2.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="NLog" Version="4.5.6" />
  </ItemGroup>
  <ItemGroup>
    <None Include="README.md" Pack="true" PackagePath="\"/>
  </ItemGroup>
</Project>